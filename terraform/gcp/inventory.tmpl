all:
  children:
    hana:
      hosts:
%{ for index, value in hana-pip ~}
        ${hana-name[index]}:
          ansible_host: ${value}
          ansible_python_interpreter: /usr/bin/python3
%{ endfor ~}
 %{ if iscsi-enabled }
    iscsi:
      hosts:
        ${iscsi-name}:
          ansible_host: ${iscsi-pip}
          ansible_python_interpreter: /usr/bin/python3
%{ endif }
  hosts: null